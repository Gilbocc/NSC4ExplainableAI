DS: MNIST, FASHION_MNIST, CIFAR, GTSRB, IMAGENET
FIND i[<<shape>>]
S.T. class(<<classNN>>(i), <<class_not_var>>),
     <<img_constraint>>,
     <<classNN>>(i).p[0, <<class_not_var>>] > 0.8,
     <<classNN>>(i).p[0, <<class_var>>] < 0.1,
     normInf(i - <<var>>) < <<adversariealInfDistance>>
INIT i = <<var>>
RETURN i